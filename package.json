  "private": true,
  "scripts": {
    "dev": "next dev",
    "db:start": "yarn supabase start",
    "db:stop": "yarn supabase stop",
    "db:status": "yarn supabase status",
    "db:types": "yarn --silent supabase gen types typescript --local > schema.gen.ts",
    "db:reset": "yarn --silent supabase db reset --local",
    "migration:generate": "yarn --silent supabase db diff -f",
    "migration:push": "yarn --silent supabase db push --local",
